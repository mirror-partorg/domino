<ui>
      <menubar name='MainMenu'>
        <menu name='File' action='File'> 
          <menuitem name='Save' action='SaveAction'/> 
          <menuitem name='SaveAs' action='SaveAsAction'/> 
          <separator/> 
          <menuitem name='PrintNow' action='PrintNowAction'/> 
          <menuitem name='Print' action='PrintAction'/> 
          <separator/> 
          <menuitem name='Close' action='CloseAction'/> 
          <menuitem name='Quit' action='QuitAction'/> 
        </menu> 
        <menu name='ViewCommand' action='ViewAction'> 
          <menuitem name='Insert' action='InsertAction'/> 
          <menuitem name='Copy' action='CopyAction'/> 
          <menuitem name='Edit' action='EditAction'/> 
          <menuitem name='Delete' action='DeleteAction'/> 
          <separator/> 
	  <menuitem name='Refresh' action='RefreshAction'/> 
          <separator/> 
          <menuitem name='Apply' action='ApplyAction'/> 
          <menuitem name='UndoApply' action='UndoApplyAction'/> 
          <menuitem name='MailSend' action='MailSendAction'/> 
          <separator/> 
	  <menuitem name='ProductStock' action='ProductEditStock'/> 
	  <menuitem name='ProductParcel' action='ProductEditParcel'/> 
	  <menuitem name='ProductPrihod' action='ProductEditPrihod'/> 
	  <menuitem name='ProductOrder' action='ProductEditOrder'/> 
	  <menuitem name='ProductCheck' action='ProductEditCheck'/> 
	  <menuitem name='ProductRz' action='ProductEditRz'/> 
	  <menuitem name='FindByBarcode' action='FindByBarcode'/> 
        </menu> 
        <menu name='Handbook' action='Handbook'> 
          <menuitem name='ProductExtended' action='ProductViewExtended'/> 
          <menuitem name='Google' action='GoogleView'/> 
          <menuitem name='IM' action='IMView'/> 
        </menu> 
        <menu name='Action' action='Action'> 
          <menuitem name='PKO' action='PKOPrint'/> 
          <menuitem name='PV'  action='PVPrint'/> 
        </menu> 
        <menu name='Tools' action='Tools'> 
          <menuitem name='CommonEdit' action='CommonEdit'/> 
        </menu> 
        <menu name='Tabs' action='Tabs'> 
          <menuitem name='TabsPrevious' action='TabsPrevious'/> 
          <menuitem name='TabsNext' action='TabsNext'/> 
          <separator/> 
          <menuitem name='TabsMoveLeft' action='TabsMoveLeft'/> 
          <menuitem name='TabsMoveRight' action='TabsMoveRight'/> 
    	   <placeholder name='TabsOpen'> 
            <separator name='TabsSep2'/> 
          </placeholder> 
        </menu> 
        <menu name='View' action='View'> 
          <menuitem name='FullScreen' action='FullScreen'/> 
          <menuitem name='MainMenu' action='MainMenu'/> 
        </menu> 
        <menu name='Help' action='HelpAction'> 
          <menuitem name='About' action='AboutAction'/> 
        </menu> 
      </menubar> 
      <toolbar name='MainToolbar'> 
        <toolitem action='GoogleViewTool'/> 
        <toolitem action='PKOPrintTool'/> 
        <toolitem action='PVPrintTool'/> 
        <separator/> 
        <toolitem action='PrintNowActionTool'/> 
        <toolitem action='PrintActionTool'/> 
        <toolitem action='ClockAction'/> 
      </toolbar> 
</ui>
